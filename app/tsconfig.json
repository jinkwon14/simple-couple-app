{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "types": ["jest", "react"]
  },
  "include": ["./App.tsx", "src", "__tests__"],
  "exclude": ["node_modules"]
}
